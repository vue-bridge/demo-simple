{
  "name": "vue3-example-library",
  "version": "0.1.0",
  "private": "true",
  "license": "MIT",
  "main": "dist/vue3-example-library.cjs.js",
  "module": "dist/vue3-example-library.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-example-library.es.js",
      "require": "./dist/vue3-example-library.cjs.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && cd vue2 && pnpm build",
    "gen-dts": "vue-tsc --emitDeclarationOnly",
    "compile": "vue-compile src -o lib",
    "compile-ts": "vue-compile src -o lib --preserve-ts-block",
    "test": "jest --config ./jest.config.js --runInBand && cd vue2 && yarn test",
    "lint": "eslint 'src/**/*.{vue,js}' --fix"
  },
  "dependencies": {
    "@vue-bridge/runtime": "^0.0.8"
  },
  "devDependencies": {
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue-bridge/eslint-config": "^0.1.0",
    "@vue-bridge/testing": "^0.0.3",
    "@vue-bridge/vite-plugin": "^0.0.6",
    "@vue/compiler-sfc": "^3.2.0",
    "@vue/test-utils": "^2.0.0-rc.14",
    "eslint": "^8",
    "eslint-plugin-vue": "^8",
    "typescript": "~4.5",
    "vite": "^2.4.0",
    "vitest": "^0.0.127",
    "vue": "^3.2.0",
    "vue-compile": "^1.1.2",
    "vue-tsc": "^0.30.1"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}
